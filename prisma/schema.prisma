// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model posts {
  id Int @id @default(autoincrement())
  author String 
  title String  
  text String 
  category String
} 

model login {
  id Int @id @default(autoincrement())
  user String
  password String
}
